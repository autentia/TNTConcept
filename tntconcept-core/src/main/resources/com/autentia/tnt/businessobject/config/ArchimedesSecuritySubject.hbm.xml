<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.autentia.tnt.businessobject.ArchimedesSecuritySubject" table="archimedes_security_subject">
        <id name="id" type="java.lang.Integer">
            <generator class="native"/>
        </id>
        <property name="principalName" type="java.lang.String" not-null="true" column="principal_name"/>
        <property name="attributes" type="java.lang.String" not-null="true"/>
    </class>
</hibernate-mapping>
