	<queryString><![CDATA[select 	orgName,
		userName,
		roleName,
		start,
		duration,
		description,
		COALESCE(fact/60,0) as fact,
		COALESCE(noFact/60,0) as noFact, 
		projectName from (

SELECT 	o.name as orgName,
		u.name as userName,
		r.name as roleName,
		p.name as projectName,	
 		a.start,
 		CASE
                   			WHEN r.isWorkingTime = 0 THEN 0
                            ELSE a.duration
                            END
                            as duration,
 		a.description,
 		sum(CASE
                     WHEN r.isWorkingTime = 0 THEN 0
                     WHEN r.isWorkingTime = 1 AND a.billable = 1 THEN duration END) as 'fact',
		sum(CASE
                     WHEN r.isWorkingTime = 0 THEN 0
                     WHEN r.isWorkingTime = 1 AND a.billable = 0 THEN duration END) as 'noFact'
		
		 
		FROM Activity a,Organization o,User u,Project p,ProjectRole r
		 
		where 	u.id=a.userId and 
				r.id=a.roleId and 
				p.organizationId=o.id and 
				p.id=r.projectId and 
				p.id=$P{Proyecto} and 
				r.id=$P{ROL} and
				a.start between $P{Fecha inicio} and
				$P{Fecha fin}
				
		group by a.id
		
		order by a.start,u.name)
		
		as tabla;]]></queryString>

