	<queryString><![CDATA[select
orgName,
userName,
startDate,
duration,
description,
COALESCE(fact/60,0) as fact,
COALESCE(noFact/60,0) as noFact,
COALESCE(impFact/60,0) as impFact from (
SELECT  o.name as orgName,
				u.name as userName,
                a.start,duration,a.description,
                sum(CASE WHEN a.billable=1 then duration end) as 'fact',
				sum(CASE WHEN a.billable=0 then duration end) as 'noFact',
                sum(CASE WHEN a.billable=1 then (duration * r.costPerHour) end) as 'impFact'
FROM Activity a,Organization o,User u,Project p,ProjectRole r
where
u.id=a.userId and
r.id=a.roleId and
p.organizationId=o.id and
p.id=r.projectId and
u.id=$P{Usuario} and
p.organizationId=$P{Organizacion} and
a.start between $P{Fecha inicio} and $P{Fecha fin}
group by o.name
order by o.name,a.start,u.name) as tabla;]]></queryString>

