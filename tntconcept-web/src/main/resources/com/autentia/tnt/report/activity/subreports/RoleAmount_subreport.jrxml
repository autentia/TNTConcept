	<parameter name="Proyecto" isForPrompting="false" class="java.lang.Integer">
		<parameterDescription><![CDATA[Project]]></parameterDescription>
		<defaultValueExpression ><![CDATA[new Integer (6)]]></defaultValueExpression>
	</parameter>
	<parameter name="Fecha inicio" isForPrompting="true" class="java.sql.Timestamp">
		<parameterDescription><![CDATA[Timestamp]]></parameterDescription>
		<defaultValueExpression ><![CDATA[new java.sql.Timestamp(new Date().getTime())]]></defaultValueExpression>
	</parameter>
	<parameter name="Fecha fin" isForPrompting="true" class="java.sql.Timestamp">
		<parameterDescription><![CDATA[Timestamp]]></parameterDescription>
		<defaultValueExpression ><![CDATA[new java.sql.Timestamp(new Date().getTime())]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[select roleName,
COALESCE(impFact/60,0) as impFact from (

	SELECT o.name as orgName,u.name as userName,r.name as roleName,p.name as projectName,
		a.start,CASE
                   			WHEN r.isWorkingTime = 0 THEN 0
                            ELSE a.duration
                            END
                            as duration,a.description,
        sum(CASE WHEN r.isWorkingTime = 0 THEN 0 WHEN r.isWorkingTime = 1 AND a.billable=1 then (duration * r.costPerHour) end) as 'impFact'
	FROM Activity a, Organization o,User u,Project p,ProjectRole r 
	where u.id=a.userId and r.id=a.roleId and p.organizationId=o.id and p.id=r.projectId 
	and p.id= $P{Proyecto} and a.start between $P{Fecha inicio} and $P{Fecha fin}
	group by r.id
	order by a.start,u.name) as tabla;]]></queryString>

